<!DOCTYPE html>

<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<header>
  <% include ./partials/_header %>
</header>
<body>
  <ul>
    <% for (const elem in urls) { %>
      <li><%= urls[elem] %>
        <a href=<%= `/urls/${elem}/` %>> Edit </a>
        <form method="POST" action=<%= `/urls/${elem}/delete` %>>
          <button>Delete</button>
        </form>
      </li>
    <% }; %>
  </ul>
  <a href='#'></a>
</body>
</html>