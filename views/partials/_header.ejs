
<header style="text-align:center">
  <h1><strong>TinyApp</strong></h1>
    <% if (!user) { %>
     <a href='/login'>Login</a>
     <a href='/register'>Register</a>
  <% } else { %>
    <h3><%= `Logged in as: ${user['email']}` %></h3>
    <form method="POST" action="/logout">
        <button>Logout</button>
    </form>
  <% } %>

</header>

